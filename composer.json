{
    "name": "surfnet/stepup-middleware",
    "license": "Apache-2.0",
    "description": "The SURFnet Step-up Middleware exposes an API for managing Step-up Identities and Tokens.",
    "autoload": {
        "psr-4": {
            "Surfnet\\StepupMiddleware\\": "src\\Surfnet\\StepupMiddleware",
            "Surfnet\\Stepup\\": "src\\Surfnet\\Stepup"
        }
    },
    "autoload-dev": {
        "classmap": ["src/Surfnet/StepupMiddleware/ApiBundle/Tests/Request/commands.php"]
    },
    "minimum-stability": "stable",
    "repositories": [{"type":"vcs","url":"https://github.com/rjkip/symfony"}],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "dev-bugfix/default-entry-point as 2.5.5",
        "doctrine/dbal": "~2.2,>=2.2.3",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "nelmio/security-bundle": "~1.4",
        "doctrine/doctrine-migrations-bundle": "~1.0.0",
        "doctrine/migrations": "~1.0.0|1.0.0@dev",
        "broadway/broadway": "~0.2.0",
        "mockery/mockery": "~0.9",
        "surfnet/stepup-bundle": "dev-develop"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "ibuildings/qa-tools": "~1.1,>=1.1.27",
        "liip/rmt": "1.1.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
