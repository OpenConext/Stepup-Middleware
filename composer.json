{
    "name": "surfnet/stepup-middleware",
    "license": "Apache-2.0",
    "description": "The SURFnet Step-up Middleware exposes an API for managing Step-up Identities and Tokens.",
    "autoload": {
        "psr-4": {
            "Surfnet\\Stepup\\": "src\\Surfnet\\Stepup",
            "Surfnet\\StepupMiddleware\\": "src\\Surfnet\\StepupMiddleware"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Surfnet\\StepupMiddleware\\Test\\Database\\": "tests\\database"
        },
        "classmap": [
            "src/Surfnet/StepupMiddleware/ApiBundle/Tests/Request/commands.php"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "ext-gmp": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "broadway/broadway": "^2.5",
        "broadway/event-store-dbal": "^0.6",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.13.2",
        "doctrine/doctrine-migrations-bundle": "^3.4.1",
        "doctrine/orm": "^3.3.2",
        "incenteev/composer-parameter-handler": "^2.2",
        "jms/translation-bundle": "^2.6",
        "nelmio/security-bundle": "^3.4.2",
        "openconext/monitor-bundle": "^4.3.1",
        "pagerfanta/pagerfanta": "^4.7.1",
        "ramsey/uuid": "^4.7.6",
        "surfnet/stepup-bundle": "^7.0",
        "symfony/asset": "7.4.*",
        "symfony/expression-language": "7.4.*",
        "symfony/flex": "^2.4.7",
        "symfony/form": "7.4.*",
        "symfony/framework-bundle": "7.4.*",
        "symfony/intl": "7.4.*",
        "symfony/mailer": "7.4.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/password-hasher": "7.4.*",
        "symfony/runtime": "7.4.*",
        "symfony/security-bundle": "7.4.*",
        "symfony/translation": "7.4.*",
        "symfony/twig-bundle": "7.4.*",
        "symfony/validator": "7.4.*",
        "twig/extra-bundle": "^3.20",
        "twig/intl-extra": "^3.20",
        "twig/twig": "^3.20"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^2.2",
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "irstea/phpcpd-shim": "^6.0",
        "liip/test-fixtures-bundle": "^3.6",
        "malukenho/docheader": "^1.1",
        "mockery/mockery": "1.7.x-dev",
        "overtrue/phplint": ">=9.5.6",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-mockery": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^10",
        "rector/rector": "^2.2",
        "slevomat/coding-standard": "^8.25",
        "squizlabs/php_codesniffer": "^4",
        "symfony/browser-kit": "7.4.*",
        "symfony/phpunit-bridge": "7.4.*",
        "symfony/stopwatch": "7.4.*",
        "symfony/web-profiler-bundle": "7.4.*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "check": [
          "@check-ci",
          "@rector",
          "composer audit"
        ],
        "check-ci": [
            "@phplint",
            "@phpmd",
            "@phpcs",
            "@phpstan",
            "@phpcpd",
            "@license-headers",
            "@phpunit"
        ],
        "phplint": "./ci/qa/phplint",
        "phpmd": "./ci/qa/phpmd",
        "license-headers": "./ci/qa/docheader",
        "rector":  "./ci/qa/rector.sh --dry-run",
        "rector-fix":  "./ci/qa/rector.sh",
        "phpstan": "./ci/qa/phpstan",
        "phpcs": "./ci/qa/phpcs",
        "phpcbf": "./ci/qa/phpcbf",
        "phpcpd": "./ci/qa/phpcpd",
        "phpunit": "./ci/qa/phpunit",

        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "@auto-scripts"
        ],
        "frontend-install":
            "echo 'Skipping task: no frontend assets available'"
    },
    "archive": {
        "exclude": [
            "*",
            ".*",
            "*/Tests/*",
            "!/component_info",
            "!/bin",
            "!/config",
            "!/public",
            "!/src",
            "!/templates",
            "!/translations",
            "!/var/cache/prod",
            "!/vendor"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "endroid/installer": false,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "platform": {
            "php": "8.2"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "incenteev-parameters": {
            "file": "config/openconext/parameters.yaml"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        },
        "runtime": {
            "disable_dotenv": true
        },
        "src-dir": "src/Surfnet"
    }
}
