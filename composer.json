{
    "name": "surfnet/stepup-middleware",
    "license": "Apache-2.0",
    "description": "The SURFnet Step-up Middleware exposes an API for managing Step-up Identities and Tokens.",
    "autoload": {
        "psr-4": {
            "Surfnet\\": "src\\Surfnet"
        }
    },
    "autoload-dev": {
        "classmap": ["src/Surfnet/StepupMiddleware/ApiBundle/Tests/Request/commands.php"]
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.6",
        "doctrine/dbal": "~2.5@beta",
        "doctrine/orm": "dev-master",
        "doctrine/doctrine-bundle": "dev-master",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "nelmio/security-bundle": "~1.4",
        "doctrine/doctrine-migrations-bundle": "~1.0.0",
        "doctrine/migrations": "~1.0.0|1.0.0@dev",
        "broadway/broadway": "~0.5.0",
        "mockery/mockery": "~0.9",
        "surfnet/stepup-bundle": "dev-develop",
        "pagerfanta/pagerfanta": "~1.0",
        "sensio/generator-bundle": "~2.3",
        "jms/translation-bundle": "~1.1",
        "jms/di-extra-bundle": "~1.5",
        "jms/aop-bundle": "~1.0"
    },
    "require-dev": {
        "ibuildings/qa-tools": "~1.1,>=1.1.27",
        "liip/rmt": "1.1.*",
        "moontoast/math": "~1.1",
        "rhumsaa/uuid": "~2.8",
        "mockery/mockery": "~0.9",
        "liip/functional-test-bundle": "~1.2",
        "doctrine/data-fixtures": "~1.1"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
